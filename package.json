  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:hasura\"",
    "dev:hasura": "cd hasura && npx hasura --skip-update-check  --envfile ../.env console --no-browser",
    "dev:api": "bun run --hot index.ts",
    "build": "tsc",
    "start": "node --es-module-specifier-resolution=node --loader ts-node/esm ./build/index.js",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "schema:fetch": "bash ./bin/fetch-gql-schema.sh",
    "cli": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm cli.ts",
    "schema:generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@bravo68web/prettier-config": "^1.1.3",
    "@bravo68web/tsconfig": "^1.1.3",
    "@graphql-codegen/cli": "^4.0.1",
